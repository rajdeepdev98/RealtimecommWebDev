# Node.js
# Ignore node_modules in both server and frontend
/server/node_modules
/frontend/node_modules

# Logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Dependency directories
/server/package-lock.json
/frontend/package-lock.json
/server/yarn.lock
/frontend/yarn.lock

# Runtime data
/server/pids
/server/*.pid
/server/*.seed
/server/*.pid.lock

# Directory for instrumented libs generated by jscoverage/JSCover
/server/lib-cov

# Coverage directory used by tools like istanbul
/server/coverage
/frontend/coverage

# TypeScript cache
/server/**/*.tsbuildinfo
/frontend/**/*.tsbuildinfo

# Optional npm cache directory
.npm

# Optional eslint cache
/server/.eslintcache
/frontend/.eslintcache

# Optional REPL history
.node_repl_history

# dotenv environment variables file
/server/.env
/frontend/.env

# Next.js build and output (if using Next.js in frontend)
.next
out

# SvelteKit build (if using SvelteKit in frontend)
.svelte-kit

# Vite build (if using Vite in frontend)
dist

# Parcel cache (if using Parcel bundler)
.cache

# Mac OS specific files
.DS_Store

# Windows specific files
Thumbs.db

# JetBrains specific files (for WebStorm, IntelliJ, etc.)
.idea/

# VSCode settings folder
.vscode/

# Ignore build outputs for server and frontend
/server/build
/server/dist
/frontend/build
/frontend/dist

# Ignore test coverage results
/server/coverage/
/frontend/coverage/

# Ignore temporary files
*.swp
*.tmp
